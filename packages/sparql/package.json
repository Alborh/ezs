{
  "name": "@ezs/sparql",
  "version": "1.0.5",
  "description": "SPARQL module for ezs",
  "main": "./lib/index.js",
  "scripts": {
    "lint": "eslint --ext=.js ./test/*.js ./src/*.js",
    "doc": "documentation readme src/* --markdown-toc-max-depth=2 --readme-file=../../docs/plugin-sparql.md --section=usage && cp ../../docs/plugin-sparql.md ./README.md",
    "build": "babel --root-mode upward src --out-dir lib",
    "prepublish": "npm run build",
    "preversion": "npm run doc"
  },
  "publishConfig": {
